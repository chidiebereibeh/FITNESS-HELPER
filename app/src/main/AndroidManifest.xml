<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="net.ingramintegrations.FitnessHelper">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FootActivity"></activity>
        <activity android:name=".KneeActivity" />
        <activity android:name=".ThighActivity" />
        <activity android:name=".WristActivity" />
        <activity android:name=".ElbowActivity" />
        <activity android:name=".ShoulderActivity" />
        <activity android:name=".NeckActivity" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".MainActivity"/>
            <activity android:name=".Statistics">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>